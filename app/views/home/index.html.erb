<h1>All User Movies</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>user_id</th>
      <th>movie_id</th>
      <th>star</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_movies.each do |user_movie| %>
      <tr>
        <td><%= user_movie.id %></td>
        <td><%= user_movie.user_id %></td>
        <td><%= user_movie.movie_id %></td>
        <td><%= user_movie.star %></td>
      </tr>
    <% end %>
  </tbody>
</table>